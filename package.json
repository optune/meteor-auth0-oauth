{
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb/base",
      "plugin:meteor/guide"
    ],
    "rules": {
      "max-lines": "warn"
    }
  },
  "keywords": [
    "meteor",
    "auth0",
    "oauth",
    "oauth2"
  ],
  "license": "MIT",
  "homepage": "https://github.com/optune/meteor-auth0-oauth",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-meteor": "^6.0.0"
  }
}
