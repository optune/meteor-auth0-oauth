{
  "scripts": {
    "lint": "eslint --ext js,jsx .",
    "prepush": "meteor npm run lint"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/optune/meteor-auth0-oauth"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "meteor",
      "prettier"
    ],
    "extends": [
      "plugin:meteor/recommended",
      "prettier"
    ],
    "rules": {
      "max-lines": "warn",
      "prettier/prettier": "error"
    }
  },
  "keywords": [
    "meteor",
    "auth0",
    "oauth",
    "oauth2"
  ],
  "license": "MIT",
  "homepage": "https://github.com/optune/meteor-auth0-oauth",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-meteor": "^6.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^1.19.1"
  },
  "dependencies": {}
}
